# Returns 1 if number n is prime; 0 else
def int isPrime(int n):
 	int i;
 	i = 2;
 	int top;
 	top = n * n;
 	if(n <= 1):
 		return 0;
 	end
 	while(i < top):
 		if (n % i == 0):
 			return 0;
 		end
 		i = i + 1;
 	end
 	return 1;
end

puts("Enter a number: ");
int number;
number = geti();
number = isPrime(number);
if (number):
	puts("Is prime!\n");
else:
	puts("Is not prime!\n");
end
